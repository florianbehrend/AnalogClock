        <attr name="borderColor" format="reference"/>
        <attr name="clockBackgroundColor" format="reference"/>
        <attr name="hourColor" format="reference"/>
        <attr name="minuteColor" format="reference"/>
        <attr name="secondColor" format="reference"/>
